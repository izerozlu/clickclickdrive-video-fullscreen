{
  "manifest_version": 3,
  "name": "ClickClickDrive Video Fullscreen",
  "version": "0.0.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "background/load-trigger.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.clickclickdrive.de/*"
      ],
      "js": [
        "content-scripts/initial-page.js"
      ]
    },
    {
      "matches": [
        "https://storage.googleapis.com/*"
      ],
      "js": [
        "content-scripts/video-page.js"
      ]
    }

  ]
}
